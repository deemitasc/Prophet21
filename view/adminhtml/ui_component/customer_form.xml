<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="address">
        <field name="prophet_21_id" formElement="input">
            <settings>
                <validation>
                    <rule name="validate-number" xsi:type="boolean">true</rule>
                </validation>
                <notice translate="true">
                    If blank or zero will fall back to customer's first ship-to ID in P21 when order is placed.
                    This will be the case for addresses saved directly on the website.
                </notice>
                <dataType>number</dataType>
                <visible>true</visible>
            </settings>
        </field>
    </fieldset>
</form>
